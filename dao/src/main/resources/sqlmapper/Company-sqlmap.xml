<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="CompanyMapper">
    <select id="queryAllCompany" resultType="com.quancheng.achilles.dao.model.CompanyDomain">
        select id,name,title from quancheng_db.16860_client where status =1
    </select>
    <select id="queryCompanyById" resultType="com.quancheng.achilles.dao.model.CompanyDomain" parameterType="int">
        select id,name,title from quancheng_db.16860_client where status =1 and id= #{id}
    </select>
</mapper>